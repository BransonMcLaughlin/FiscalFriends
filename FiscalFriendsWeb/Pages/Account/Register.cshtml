@page
@model FiscalFriendsWeb.Pages.Account.Register
@{
    ViewData["Title"] = "Register";
}
<form method="post">
    <div class="row m-2">
        <div class="col-2">
            
            <label asp-for="newUser.FirstName" class="form-label"></label>
        </div>
        <div class="col-10">
            <input asp-for="newUser.FirstName" type="text" class="form-control"/>
        </div>
    </div>
    <div class="row m-2">
        <div class="col-2">

            <label asp-for="newUser.LastName" class="form-label"></label>
        </div>
        <div class="col-10">
            <input asp-for="newUser.LastName" type="text" class="form-control" />
        </div>
    </div>
    <div class="row m-2">
        <div class="col-2">

            <button id="btnRegister" class="btn btn-primary">Register</button>>
        </div>
        
    </div>
</form>